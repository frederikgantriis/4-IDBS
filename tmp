
SELECT 'Projects: ID --> PID' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM ( 
	SELECT P.ID
	FROM Projects p
	GROUP BY P.ID
	HAVING COUNT(DISTINCT P.PID) > 1
) X;


SELECT 'Projects: ID --> SID' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM ( 
	SELECT P.ID
	FROM Projects p
	GROUP BY P.ID
	HAVING COUNT(DISTINCT P.SID) > 1
) X;


